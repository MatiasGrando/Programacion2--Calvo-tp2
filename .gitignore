#include "Nodo.h"

Nodo::Nodo(unsigned int jugador)
{
        this->jugador = jugador;
        this->energia = ENERGIAMAXIMA;

}

Nodo* Nodo::obtenerSiguiente()
{
    return this->nodoSiguiente;
}

Nodo* Nodo::obtenerAnterior()
{
    return this->nodoAnterior;
}

void Nodo::disminuirEnergia(unsigned int energiaPerdida)
{
    this->energia= (this->energia - energiaPerdida);
}

int Nodo::obetenerJugador()
{
    return this->jugador;
}

int Nodo::obtenerEnergia()
{
    return this->energia;
}
